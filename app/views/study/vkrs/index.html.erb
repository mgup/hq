<div class="page-header">
  <h1>Выпускные квалификационные работы</h1>
</div>

<% @groups.sort_by(&:name).each do |group| %>
  <h2><%= group.name %></h2>

  <% group.students.for_vkr.each do |student| %>
    <div id="vkr<%= student.id %>">
      <%= render partial: 'study/vkrs/vkr', locals: { group: group, student: student } %>
    </div>
  <% end %>
<% end %>
